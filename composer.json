{
    "name": "tenqz/lingua",
    "description": "Lingua is a comprehensive PHP library designed for advanced text processing.",
    "type": "library",
    "keywords": [
        "text",
        "text-processing",
        "text-analysis",
        "string",
        "string-processing",
        "nlp",
        "lemmatization",
        "stemming",
        "stemmer",
        "morphology",
        "tokenization",
        "text-normalization"
    ],
    "homepage": "https://github.com/tenqz/lingua",
    "license": "MIT",
    "authors": [
        {
            "name": "Oleg Patsay",
            "email": "smmartbiz@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Tenqz\\Lingua\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tenqz\\Lingua\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "scripts": {
        "test": "phpunit"
    }
}
